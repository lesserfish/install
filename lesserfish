
server {
        listen 80 default_server;
        server_name lesserfish.dev;

        return 404;

        location / {
                proxy_redirect off;
                proxy_set_header        Host $host;
                proxy_set_header        X-Real-IP $remote_addr;
                proxy_set_header        X-Forwarded-For $proxy_add_x_forwarded_for;
                proxy_set_header        X-Forwarded-Proto $scheme;
                proxy_pass          http://localhost:3000;
        }
}

server {
        listen 80;
        server_name ame.lesserfish.dev amekanji.com www.amekanji.com;

        location / {
                proxy_redirect off;
                proxy_set_header        Host $host;
                proxy_set_header        X-Real-IP $remote_addr;
                proxy_set_header        X-Forwarded-For $proxy_add_x_forwarded_for;
                proxy_set_header        X-Forwarded-Proto $scheme;
                proxy_pass          http://localhost:9000;
        }
}

server {
        listen 80;
        server_name lafoxtex.lesserfish.dev lafoxtex.com www.lafoxtex.com;

        location / {
                proxy_redirect off;
                proxy_set_header        Host $host;
                proxy_set_header        X-Real-IP $remote_addr;
                proxy_set_header        X-Forwarded-For $proxy_add_x_forwarded_for;
                proxy_set_header        X-Forwarded-Proto $scheme;
                proxy_pass          http://localhost:9100;
        }
}
server {
        listen 80;
        server_name auto.lesserfish.dev;

        include /etc/nginx/mime.types;

        root /var/www/Auto/;

        location / {
        }
}

server {
        listen 80;
        server_name urm.lesserfish.dev;
        root                    /var/www/URM;
        index                   index.html;
        client_max_body_size    16m;
}
